    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-router-hash-link": "^2.4.5",
    "@types/video.js": "^7.3.49",
    "@types/videojs-mobile-ui": "^0.5.0",
    "@types/videojs-seek-buttons": "^2.1.0",
